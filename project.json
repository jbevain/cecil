{
    "version": "0.9.6.0",
    "author": "Jb Evain",
    "description": "Cecil is a library to generate and inspect programs and libraries in the ECMA CIL form",

    "compilationOptions": {
        "define": ["NET_CORE", "NET_4_0", "NET_3_5", "NET_2_0"]
    },
    "exclude": [
      "Test",
      "rocks",
      "symbols"
    ],

    "frameworks" : {
        "net45" : {},
        "netstandard1.5" : {
            "dependencies": {
                "NETStandard.Library": "1.5.0-rc2-23925",
                "System.IO": "4.1.0-rc2-24022",
                "System.Reflection": "4.1.0-rc2-24022",
                "System.Reflection.TypeExtensions": "4.1.0-rc2-24022",
                "System.Reflection.Primitives": "4.0.1-rc2-24022",
                "System.Runtime": "4.1.0-rc2-24022",
                "System.Runtime.Extensions": "4.1.0-rc2-24022",
                "System.Threading": "4.0.11-rc2-24022",
                "System.Security.Cryptography.Primitives": "4.0.0-rc2-24022",
                "System.Security.Cryptography.Algorithms": "4.1.0-rc2-24022"
            },
            "imports": [
                "portable-net45+win8",
                "dnxcore50"
            ]
        }
    }
}
